#!/bin/bash
dirname=`dirname $0`

$dirname/elget
case $? in
    # error
    -1)
    exit 1
    ;;

    # binaries downloaded successfully
    0)
    $dirname/elget -C debug
    exit 0
    ;;

    # binaries not found, build locally instead
    1)
    $dirname/elb configure debug release
    exit $?
    ;;
esac